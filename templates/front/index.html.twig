    <div class="parallax" data-stellar-background-ratio="0.5" style="background-image: url('{{ asset('images/parallax.jpg') }}')">
        <iframe allow="autoplay" frameborder="no" height="250" scrolling="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/playlists/811932789&color=%23ff5500&auto_play=false&hide_related=false&show_comments=true&show_user=true&show_reposts=false&show_teaser=true&visual=true" width="40%"></iframe>
    </div>


    <div class="career" id="career">
        <div class="career-ctn-title">
            <h2>CAREER</h2>
        </div>
        <div class="career-ctn-center">
            <h2 class="career-responsive-title">CAREER</h2>
            <div class="career-ctn-center-gallery-wrap">
                {% set delayExp = 0 %}
                {% for exp in exps %}
                    {% set delayExp = delayExp + 0.1 %}
                    <div class="item" style="background-image: url('{{ vich_uploader_asset(exp, 'imageFile') }}'); animation-delay:{{ delayExp }}s">
                        <div class="item-ctn-desc">
                            <h3>{{ exp.title }}</h3>
                            <h4>{{exp.place}}
                                /
                                {{exp.city}}</h4>
                            <h4>{{exp.dateBegin}}
                                /
                                {{exp.dateEnd}}</h4>
                            <p>{{exp.description}}</p>
                        </div>
                        <div class="item-ctn-title">
                            <h2>{{exp.city}}</h2>
                        </div>
                        <div class="career-btn-reponsive">
                            <i class="fas fa-chevron-down"></i>
                        </div>
                    </div>
                {% endfor %}
            </div>
        </div>
    </div>

    <div class="portfolio" id="portfolio">
        <div class="portfolio-ctn-title">
            <h2>PORTFOLIO</h2>
        </div>
        <div class="portfolio-ctn-center">
            <h2 class="portfolio-responsive-title">PORTFOLIO</h2>
            <div class="portfolio-ctn-center-gallery-wrap">
                {% set delayPro = 0 %}
                {% for project in projects %}
                    {% set delayPro = delayPro + 0.1 %}
                    <div class="portfolio-card" style="animation-delay:{{ delayPro }}s">
                        <div class="portfolio-card-img" style="background-image: url('{{ vich_uploader_asset(project, 'imageFile') }}')"></div>
                        <h3>{{ project.title }}</h3>
                        <div class="portfolio-card-btn btn" id="{{ project.id }}" data-toggle="modal" data-target="#Modal">LEARN MORE</div>
                    </div>
                {% endfor %}
            </div>
        </div>
        <svg class="load" enable-background="new 0 0 500 500" version="1.1" viewbox="0 0 500 500" x="0px" xml:space="preserve" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns="http://www.w3.org/2000/svg" y="0px">
            <polygon class="load-triangle" fill="#EFEFEF" points="76.3,424.5 250.5,76.1 424.7,424.5 	"/>
        </svg>
        <div aria-labelledby="exampleModalLabel" class="modal fade" id="Modal" role="dialog" tabindex="-1">
            <div class="modal-dialog modal-lg" role="document">
                <div class="modal-content"></div>
            </div>
        </div>
    </div>
    <div class="contact" style="background-image: url('{{ asset('images/contact.jpg') }}')">
    </div>

{% endblock %}
